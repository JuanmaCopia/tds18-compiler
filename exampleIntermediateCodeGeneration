program
begin

	integer functionX(integer z, integer w)
		begin
			return z+w;
		end

	integer main()
		begin
			integer result;
			result = functionX(5, 10);
            if (result > 10) then begin
                return 1;
            end
            else begin
                return 0;
            end
		end
end

    BEGINF main
    PUSH t0
    PUSH t1
    CALL functionX t2
    ASGN result t2
    ENDF main

    BEGINF functionX:
    ADD t3 z w
    RETURN t3
    ENDF functionX
