program
begin

	integer functionX(integer z, integer w)
		begin
			return z+w;
		end

	integer main()
		begin
			integer result;
			result = functionX(5, 10);
            if (result > 10) then begin
                return 1;
            end
            else begin
                return 0;
            end
		end
end

funcion main:
    TMP2 t0 5
    TMP2 t1 10
    PUSH t0
    PUSH t1
    CALL functionX t2
    ASGN result t2
    ENDF main

funcion functionX:
    ADD t3 z w
    RTRN t3
    ENDF functionX
